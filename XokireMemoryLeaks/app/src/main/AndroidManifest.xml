<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="xplore.br.com.xokirememoryleaks">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".XploreMemoryLeakHandler"
            android:label="@string/xplore_handler_memory_leak"></activity>
        <activity
            android:name=".XploreMemoryLeakWorkerThread"
            android:label="@string/xplore_worker_thread_memory_leak">
        </activity>
        <activity android:name=".XploreMemoryLeakAsyncTask"
            android:label="@string/xplore_asynctask_memory_leak">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>